	<query language="SQL"><![CDATA[SELECT 
    p.pay_id AS 'Payment ID', 
    p.amount AS 'Payment Amount', 
    p.date AS 'Payment Date', 
    p.method AS 'Payment Method', 
    p.tax AS 'Tax Amount', 
    p.Discount AS 'Discount Amount', 
    r.rent_id AS 'Rent ID'
FROM 
    Payment p
JOIN 
    Rent_Payment_Details rpd ON p.pay_id = rpd.pay_id
JOIN 
    Rent r ON rpd.rent_id = r.rent_id;
]]></query>
