	<query language="SQL"><![CDATA[SELECT 
    vr.vehicle_id AS 'Vehicle ID', 
    v.model AS 'Model', 
    v.colour AS 'Colour', 
    r.start_date AS 'Rental Start Date', 
    r.end_date AS 'Rental End Date', 
    vr.rent_date AS 'Rent Date', 
    vr.cost AS 'Total Cost'
FROM 
    Vehicle_Rent_Details vr
JOIN 
    Vehicle v ON vr.vehicle_id = v.vehicle_id
JOIN 
    Rent r ON vr.rent_id = r.rent_id;
]]></query>
